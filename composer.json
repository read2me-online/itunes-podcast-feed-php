{
  "name": "read2me-online/itunes-podcast-feed-php",
  "type": "library",
  "description": "A tiny PHP library for generating iTunes-compatible podcast feeds. ",
  "keywords": ["itunes","podcast feed", "rss"],

  "require": {
    "php": ">=7.1"
  },

  "autoload": {
    "psr-4": {
      "iTunesPodcastFeed\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "iTunesPodcastFeed\\Tests\\": "tests/"
    }
  },

  "require-dev": {
    "phpunit/phpunit": "^7.0"
  }
}